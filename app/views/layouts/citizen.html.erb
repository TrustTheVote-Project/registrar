<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
  <head>
  <title>Jeffersonia Secretary of State</title>
    <%= stylesheet_link_tag 'http://yui.yahooapis.com/2.6.0/build/reset-fonts-grids/reset-fonts-grids' %>
    <%= stylesheet_link_tag 'http://yui.yahooapis.com/2.6.0/build/base/base' %>
    <%= stylesheet_link_tag 'application' %>
    <%= javascript_include_tag :defaults %>
    <%= stylesheet_link_tag 'citizen_landing' %>
    
    <%= yield :head %>
  </head>
  <body>
  <div id="doc2">
    <div id="hd">
      <ul>
        <li class="first"><%= link_to image_tag('navigation/home.png'), home_path %></li>
        <li><%= link_to image_tag('navigation/register_to_vote.png'), citizen_landing2_path %></li>
        <li><%= link_to image_tag('navigation/contacts.png') %></li>
        <li><%= link_to image_tag('navigation/secretary_of_state.png') %></li>
        <li><%= link_to image_tag('navigation/counties.png') %></li>
      </ul>
    </div>
    <div id="bd">
      <div id="yui-main">
        <% if @box_title %>
          <div id="box_title"><%= @box_title %></div>
        <% end %>
        <div class="yui-b">
          <%= yield %>
        </div>
      </div>
    </div>
    <div id="ft">
      <%= link_to 'About' %>
      <%= link_to 'Privacy Policy' %>
      <%= link_to 'Contacts' %>
      <%= link_to 'Feedback' %>
      <%= link_to 'Copyright 2008' %>
    </div>
  </div>
  </body>
</html>